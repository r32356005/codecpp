#include <iostream> 
using namespace std; 
int main(){ 
  int t; 
  cin>>t; 
  for(int i=0;i<t;i++){ 
    long long int sum=0,a; 
    cin>>a; 
    while(a>=13){ 
      a=a-13; 
      sum=sum+1; 
    } 
    while(a>=8){ 
      a=a-8; 
      sum=sum+1; 
    } 
    while(a>=5){ 
      a=a-5; 
      sum=sum+1; 
    } 
    while(a>=3){ 
      a=a-3; 
      sum=sum+1; 
    } 
    while(a>=2){ 
      a=a-2; 
      sum=sum+1; 
    } 
    while(a>=1){ 
      a=a-1; 
      sum=sum+1; 
    } 
    cout<<sum<<endl; 
  } 
  return 0; 
}  
